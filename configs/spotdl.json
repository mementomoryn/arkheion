{
    "client-id": "[[SPOTDL_CLIENT_ID]]",
    "client-secret": "[[SPOTDL_CLIENT_SECRET]]",
    "no-cache": true,
    "audio": [
        "youtube-music",
        "bandcamp",
        "soundcloud",
        "youtube"
    ],
    "lyrics": [
        "musixmatch",
        "synced",
        "genius",
        "azlyrics"
    ],
    "output": "music/{album-artist}/{album} \({year}\)/{artist} \- {title}.{output-ext}",
    "format": "opus",
    "filter-results": false,
    "threads": 4,
    "print-errors": true,
    "sponsor-block": [
        "sponsor",
        "intro",
        "outro",
        "selfpromo",
        "preview",
        "filler",
        "interaction"
    ],
    "preload": true,
    "load-config": false,
    "log-level": "DEBUG",
    "generate-lrc": true
}