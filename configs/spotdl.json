{
    "client_id": "[[SPOTDL_CLIENT_ID]]",
    "client_secret": "[[SPOTDL_CLIENT_SECRET]]",
    "no_cache": true,
    "audio_providers": [
        "youtube-music",
        "bandcamp",
        "soundcloud",
        "youtube"
    ],
    "lyrics_providers": [
        "musixmatch",
        "synced",
        "genius",
        "azlyrics"
    ],
    "output": "music/{album-artist}/{album} ({year})/{artist} - {title}.{output-ext}",
    "format": "opus",
    "filter_results": false,
    "threads": "auto",
    "cookie_file": null,
    "print_errors": true,
    "sponsor_block": [
        "sponsor",
        "intro",
        "outro",
        "selfpromo",
        "preview",
        "filler",
        "interaction"
    ],
    "preload": true,
    "load_config": false,
    "log_level": "DEBUG",
    "generate_lrc": true
}